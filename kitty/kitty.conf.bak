tab_bar_edge top
tab_bar_style powerline 
tab_powerline_style slanted 

window_padding_width 2

foreground #dddddd
background #01050E




# BEGIN_KITTY_FONTS
font_family      family="JetBrainsMono Nerd Font"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
symbol_map U+E0A0-U+E0A3,U+E0C0-U+E0C7 PowerlineSymbols
